
mga_tao = ["Benyo","Balbin","Clerigo"]
mga_tanga = ["Ako","Ikaw","Sila"]
mga_room =["PH","OH","MAH"]

for x in range(len(mga_tao)):
    print(mga_tao[x])

    for y in range(len(mga_tanga)):
        print(mga_tanga[y])

        for z in range(len(mga_room)):
            print(mga_room[z])

print(len(mga_tao))
print(len(mga_tanga))
print(len(mga_room))
