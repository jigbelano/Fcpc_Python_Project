#1
print("----------------------")
for x in range(1, 17, 3):
    print(x, end = '\t')
print()
#2
print("----------------------")
for x in range(100, 78, -7):
    print(x, end = '\t')
print()
#3
print("----------------------")
for x in range(1,5):
    if x % 3 == 0:
        print("1", end = '\t')
    else:
        print("0", end = '\t')
